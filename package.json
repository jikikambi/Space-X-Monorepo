{
  "name": "space-x-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "src/gateway",
    "src/react-app",
    "src/vue-app"
  ],
  "scripts": {
    "dev:gateway": "npm run dev --workspace=src/gateway",
    "dev:react": "npm run dev --workspace=src/react-app",
    "dev:vue": "npm run dev --workspace=src/vue-app",
    "dev:react:all": "concurrently \"npm run build:tailwind:react\" \"npm run dev:react\"",
    "dev:vue:all": "concurrently \"npm run build:tailwind:vue\" \"npm run dev:vue\"",
    "dev": "concurrently \"npm:dev:gateway\" \"npm:dev:react:all\" \"npm:dev:vue:all\"",
    "build": "npm run build --workspaces",
    "build:tailwind:vue": "tailwindcss -c tailwind.config.js -i ./src/styles/tailwind-input.css -o ./src/vue-app/src/assets/tailwind.css --watch",
    "build:tailwind:react": "tailwindcss -c tailwind.config.js -i ./src/styles/tailwind-input.css -o ./src/react-app/src/assets/tailwind.css --watch",
    "test:gateway": "npm --workspace=src/gateway test",
    "test:react": "npm --workspace=src/react-app test",
    "test:vue": "npm --workspace=src/vue-app test",
    "test": "npm run test:gateway && npm run test:react && npm run test:vue",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "amqplib": "^0.10.9",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.10.0",
    "body-parser": "^2.2.0",
    "bootstrap": "^5.3.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "express": "^4.21.2",
    "graphql": "^16.11.0",
    "identity-obj-proxy": "^3.0.0",
    "JSONStream": "^1.3.5",
    "mongodb": "^6.19.0",
    "node-fetch": "^3.3.2",
    "path": "^0.12.7",
    "pino": "^9.7.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "redis": "^4.7.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/vue": "^8.1.0",
    "@types/amqplib": "^0.10.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/redis": "^4.0.10",
    "@types/supertest": "^6.0.3",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.0.0",
    "jest": "^30.0.5",
    "jest-junit": "^16.0.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "tailwindcss-cli": "^0.1.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
